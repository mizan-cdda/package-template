{
    "name": "package-template",
    "version": "1.0.0",
    "description": "Package description",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js",
            "require": "./dist/*.js"
        }
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc",
        "dev": "tsc --watch",
        "clean": "pnpm exec rimraf dist",
        "prepare": "pnpm build"
    },
    "keywords": [
        "keyword1",
        "keyword2",
        "keyword3"
    ],
    "author": "author-name",
    "license": "MIT",
    "dependencies": {
        "better-auth": "^1.3.26"
    },
    "devDependencies": {
        "@types/node": "^24.5.2",
        "typescript": "^5.9.3"
    }
}